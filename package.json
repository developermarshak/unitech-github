{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:contracts": "turbo run build --filter=@repo/contracts",
    "build:api-client": "turbo run build --filter=@repo/api-client",
    "build:api": "turbo run build --filter=api",
